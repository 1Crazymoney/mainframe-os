{
  "name": "@mainframe/daemon",
  "version": "0.1.0",
  "main": "lib/index.js",
  "license": "MIT",
  "bin": {
    "mainframe-daemon": "./bin/run"
  },
  "files": ["/bin", "/lib"],
  "oclif": {
    "commands": "./lib/cli/commands",
    "bin": "mainframe-daemon",
    "plugins": ["@oclif/plugin-help"]
  },
  "scripts": {
    "clean": "del lib",
    "build:js": "babel src --out-dir lib",
    "build:flow": "flow-copy-source src lib",
    "build": "yarn clean && yarn build:js && yarn build:flow",
    "test": "flow check",
    "dev": "babel-node src",
    "start": "node -r esm lib"
  },
  "dependencies": {
    "@mainframe/rpc-node": "^0.2.0",
    "@mainframe/utils-base64": "^0.1.0",
    "@mainframe/utils-crypto": "^0.1.0",
    "@mainframe/utils-id": "^0.1.0",
    "@oclif/command": "^1.4.30",
    "@oclif/config": "^1.6.21",
    "@oclif/plugin-help": "^1.2.11",
    "asar": "^0.14.3",
    "debug": "^3.1.0",
    "es6-error": "^4.1.1",
    "esm": "^3.0.39",
    "fast-json-stable-stringify": "^2.0.0",
    "fs-extra": "^6.0.1",
    "rxjs": "^6.2.0",
    "sodium-native": "^2.1.6"
  },
  "devDependencies": {
    "flow-bin": "^0.73.0"
  }
}
