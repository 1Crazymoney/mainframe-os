{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 8
        },
        "modules": false,
        "loose": true
      }
    ],
    "flow"
  ],
  "plugins": [
    "syntax-class-properties",
    "syntax-object-rest-spread",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": ["env", "flow"]
    }
  }
}
